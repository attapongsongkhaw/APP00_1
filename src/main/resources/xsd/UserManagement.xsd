<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
targetNamespace="http://umm.corp.kbank.com/schema/UserManagement"
    xmlns:tns="http://umm.corp.kbank.com/schema/UserManagement"
    xmlns:ns1="http://umm.corp.kbank.com/schema/UMMCommon"
    xmlns:ns2="http://umm.corp.kbank.com/schema/CompanyManagement"
    elementFormDefault="qualified" >

    <xsd:import schemaLocation="UMMCommon.xsd" namespace="http://umm.corp.kbank.com/schema/UMMCommon"/>
    <xsd:import schemaLocation="CompanyManagement.xsd" namespace="http://umm.corp.kbank.com/schema/CompanyManagement"/>

    <xsd:complexType name="UserProfileBase">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="user_id" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="company_id" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="1" name="company_code" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="1" name="login_account" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="prefix_login_account" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="suffix_login_account" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="gender" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="salutation" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="firstname" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="lastname" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="ref_type" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="ref_no" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="email" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="phone" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="mobile" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="status" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="user_type" type="xsd:string"></xsd:element>
	    <xsd:element minOccurs="0" name="token_type" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="token" type="ns1:TokenBase"></xsd:element>
            <xsd:element minOccurs="0" name="contactpointlist" type="ns1:Contactpoints"></xsd:element>
            <xsd:element minOccurs="0" name="servicelist" type="ns1:Services"></xsd:element>
            <xsd:element minOccurs="0" name="action_mode" type="ns1:Action"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="CreateUserReqBase">
                <xsd:sequence>
                    <xsd:element name="UMMHeader" type="ns1:UMMRequestBase"></xsd:element>
                    <xsd:element name="UserProfile" type="tns:UserProfileBase"></xsd:element>
                </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CreateUserResBase">
         <xsd:complexContent>
            <xsd:extension xmlns:tns="http://umm.corp.kbank.com/schema/UMMCommon" base="ns1:UMMResponseBase">
                <xsd:sequence/>
            </xsd:extension>
         </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ModifyUserReqBase">
         <xsd:sequence>
            <xsd:element name="UMMHeader" type="ns1:UMMRequestBase"></xsd:element>
            <xsd:element name="UserProfile" type="tns:UserProfileBase"></xsd:element>
         </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ModifyUserResBase">
        <xsd:complexContent>
            <xsd:extension xmlns:tns="http://umm.corp.kbank.com/schema/UMMCommon" base="ns1:UMMResponseBase">
                <xsd:sequence/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="DeleteUserReqBase">
         <xsd:sequence>
            <xsd:element name="UMMHeader" type="ns1:UMMRequestBase"></xsd:element>
            <xsd:element name="UserProfile" type="tns:UserProfileBase"></xsd:element>
         </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DeleteUserResBase">
        <xsd:complexContent>
            <xsd:extension xmlns:tns="http://umm.corp.kbank.com/schema/UMMCommon" base="ns1:UMMResponseBase">
                <xsd:sequence/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="CheckDeleteUserReqBase">
         <xsd:sequence>
            <xsd:element name="UMMHeader" type="ns1:UMMRequestBase"></xsd:element>
            <xsd:element minOccurs="0" name="user_id" type="xsd:string"></xsd:element>
            <xsd:element name="login_account" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="company_id" type="xsd:string"></xsd:element>
            <xsd:element minOccurs="0" name="company_code" type="xsd:string"></xsd:element>
         </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CheckDeleteUserResBase">
        <xsd:complexContent>
            <xsd:extension xmlns:tns="http://umm.corp.kbank.com/schema/UMMCommon" base="ns1:UMMResponseBase">
                <xsd:sequence/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="CreateUserReq" type="tns:CreateUserReqBase"></xsd:element>
    <xsd:element name="CreateUserRes" type="tns:CreateUserResBase"></xsd:element>
    <xsd:element name="ModifyUserReq" type="tns:ModifyUserReqBase"></xsd:element>
    <xsd:element name="ModifyUserRes" type="tns:ModifyUserResBase"></xsd:element>
    <xsd:element name="DeleteUserReq" type="tns:DeleteUserReqBase"></xsd:element>
    <xsd:element name="DeleteUserRes" type="tns:DeleteUserResBase"></xsd:element>
    <xsd:element name="CheckDeleteUserReq" type="tns:CheckDeleteUserReqBase"></xsd:element>
    <xsd:element name="CheckDeleteUserRes" type="tns:CheckDeleteUserResBase"></xsd:element>

</xsd:schema>


