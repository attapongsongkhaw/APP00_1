	<!-- Service Definition -->

    <wsdl:portType name="LMSProfileService">
    
        <wsdl:operation name="createUser"  >
            <wsdl:input message="inMessageUserCreate" />
            <wsdl:output message="outMessageUserCreate" />
        </wsdl:operation>
        <wsdl:operation name="modifyUser"  >
            <wsdl:input message="inMessageUserModify" />
            <wsdl:output message="outMessageUserModify" />
        </wsdl:operation>
        <wsdl:operation name="deleteUser"  >
            <wsdl:input message="inMessageUserDelete" />
            <wsdl:output message="outMessageUserDelete" />
        </wsdl:operation>
        <wsdl:operation name="checkDeleteUser"  >
            <wsdl:input message="inMessageUserCheckDelete" />
            <wsdl:output message="outMessageUserCheckDelete" />
        </wsdl:operation>
        
