        </wsdl:operation>
        <wsdl:operation name="checkDeleteCompany"  >
            <wsdl:input message="inMessageCompanyCheckDelete" />
            <wsdl:output message="outMessageCompanyCheckDelete" />
        </wsdl:operation>
                
    </wsdl:portType>

